  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:app": "react-scripts test --watchAll=false src/App.test.js",
    "test:header": "react-scripts test --watchAll=false src/components/Header/Header.test.js",
    "test:layout": "react-scripts test --watchAll=false src/components/Layout/Layout.test.js",
    "test:home": "react-scripts test --watchAll=false src/components/Home/Home.test.js",
    "test:err404": "react-scripts test --watchAll=false src/components/Err404/Err404.test.js",
