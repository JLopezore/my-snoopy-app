    "test:todoitem": "react-scripts test --watchAll=false src/components/TodoItem/TodoItem.test.js",
    "test:todolist": "react-scripts test --watchAll=false src/components/TodoList/TodoList.test.js",
    "test:complete": "react-scripts test --watchAll=false src/components/TodoComplete/TodoComplete.test.js",
