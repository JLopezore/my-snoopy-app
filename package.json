    "test:userdir": "react-scripts test --watchAll=false src/components/UserDirectory/UserDirectory.test.js",
    "test:welcome": "react-scripts test --watchAll=false src/components/Welcome/Welcome.test.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
